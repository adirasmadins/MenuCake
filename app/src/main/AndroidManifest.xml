<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.github.thiagonascimento.com.menucake">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BoloBanana">
        </activity>
        <activity android:name=".BoloChocolate">
        </activity>
        <activity android:name=".BoloMorango">
        </activity>
        <activity android:name=".Rocambole">
        </activity>
        <activity android:name=".Cupcake">
        </activity>
    </application>

</manifest>